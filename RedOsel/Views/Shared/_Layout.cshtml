
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Security-Policy" content="
        default-src 'self';
        script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.jsdelivr.net/npm/flatpickr;
        style-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.3/css/bootstrap.min.css https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.3/css/ https://cdn.linearicons.com/free/1.0.0/ https://cdn.jsdelivr.net/npm/bootstrap-icons@@1.11.3/ https://cdn.jsdelivr.net/npm/flatpickr/dist/ https://cdnjs.cloudflare.com/ajax/libs/luxon/3.5.0/;
        style-src-elem 'self' 'unsafe-inline' https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.3/css/bootstrap.min.css https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.3/css/ https://cdn.linearicons.com/free/1.0.0/ https://cdn.jsdelivr.net/npm/bootstrap-icons@@1.11.3/ https://cdn.jsdelivr.net/npm/flatpickr/dist/ https://fonts.googleapis.com/;
        img-src 'self' data: https://www.redosel.com/archivos2014/productos/ https://redosel.com/admin/archivos/multimedia/productos/ https://maps.gstatic.com/mapfiles/ https://maps.googleapis.com/maps/ https://maps.gstatic.com/;
        font-src 'self' https://cdn.linearicons.com/free/1.0.0/ https://cdn.jsdelivr.net/npm/bootstrap-icons@@1.11.3/ https://cdn.jsdelivr.net/npm/flatpickr https://fonts.gstatic.com/;
        script-src-elem 'self' https://cdn.jsdelivr.net/npm/flatpickr https://cdnjs.cloudflare.com/ajax/libs/luxon/3.5.0/ https://maps.googleapis.com/maps/api/ https://maps.googleapis.com/maps-api-v3/api/js/61/10d/intl/es_ALL/ https://maps.googleapis.com/maps/;

        connect-src 'self' https://maps.googleapis.com/maps/api/mapsjs/ https://maps.googleapis.com/$rpc/google.internal.maps.mapsjs.v1.MapsJsInternalService/;
        frame-src 'self';
        object-src 'none';
    ">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Red Osel - @ViewBag.Title</title>
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@@1.11.3/font/bootstrap-icons.min.css">
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Icons menu CSS -->
    <link rel="stylesheet" href="~/lib/administracion/font/css/pe-icon-7-stroke.css" asp-append-version="true">
    <!-- Optional - Adds useful class to manipulate icon font display -->
    <link rel="stylesheet" href="~/lib/administracion/font/css/helper.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/administracion/font/css/style.css" asp-append-version="true">
    <link href="~/css/main.css" asp-append-version="true" rel="stylesheet" />

    

    @* <script type="application/javascript" async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD8gOoDYkNxX966vjCQzNAIH2ZHKpkBql4" asp-append-version="true"></script> *@
    @* produccion *@
    <script type="application/javascript" async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyChRCwfW3eIPONHkAgONAv2RsRy87v2OvI" asp-append-version="true"></script>
</head>
<body >
    <div class="text-center cargando-c fondoCargando-2">
        <div class="loader">
            <div class="container-c loader">
                <div class="half"></div>
                <div class="half"></div>
            </div>
        </div>
    </div>




    <section id="appLayout">
        <nav class="navbar navbar-light bg-light nav-header" v-cloak>
            <a class="navbar-brand " href="#">
                <img class="img-fluid w-100 d-none d-sm-block" src="~/lib/administracion/images/logo.png" />
                <img class="img-fluid w-100 d-block d-sm-none" src="~/lib/administracion/images/isologo-color.png" />
            </a>
            <div class="dropdown">
                <a class="nav-link dropdown-toggle p-0" href="#" id="navbarUser" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <ul class="list-inline mb-0">
                        <li class="list-inline-item icon-wrapper">
                            <img src="~/lib/administracion/images/nophoto.png" class="img-fluid rounded-circle">
                        </li>
                        <li class="list-inline-item text-wrapper ">
                            <strong>{{usuario}}</strong>
                            <p>{{perfil}}</p>
                        </li>
                    </ul>
                </a>
                <div class="dropdown-menu text-drop" aria-labelledby="navbarDropdownMenuLink">
                    <a class="dropdown-item" href="#" onclick="Salir()"><i class="lnr lnr-exit"></i> Cerrar Sesión</a>
                </div>
            </div>

            <button class="navbar-toggler btn btn-menu d-lg-none " type="button" data-toggle="collapse" data-target="#sidebar" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="lnr lnr-text-align-left"></i>
            </button>


        </nav>



        <div class="wrapper">
            <!-- Sidebar  -->
            <div id="sidebar" class="active overflow-auto" @@mouseleave="desactivaMenu">
                <div id="ItemsNav">
                    <img class="vl img-fluid" src="~/lib/administracion/images/logo-inverse.png" />
                    <img class="vlm img-fluid" src="~/lib/administracion/images/isologo.png" />
                    <p class="vl">Bienvenidos</p>

                    <ul class="list-unstyled components" id="items-menu">
                        <li v-for="(menuItem, index) in array.menu" :key="menuItem.idOpcion">
                            <!-- Parent Menu Item with Submenu -->
                            <template v-if="menuItem.idOpcionPadre === 0 && menuItem.subopciones.length > 0">
                                <div class="accordion" id="accordionItems">
                                    <button type="button"
                                            class="btn btn-link btn-block text-left btn-collaps"
                                            data-toggle="collapse"
                                            @@click="activaMenu(index)"
                                            :data-target="'#Submenu_' + index"
                                            aria-expanded="true"
                                            aria-controls="Submenu">
                                        <i :class="menuItem.icono + ' vsm-icon'"></i>
                                        <span>{{ menuItem.descripcion }}</span>

                                        <ul class="collapse list-unstyled Submenu"
                                            :id="'Submenu_' + index"
                                            aria-labelledby="headingOne"
                                            data-parent="#accordionItems">
                                            <li class="submenu-link"
                                                v-for="(subItem, subIndex) in menuItem.subopciones"
                                                :key="subItem.idOpcion">
                                                <a href="#" @@click="irAMenu(subItem.controller, subItem.pagina)">
                                                    <span class="submenu">{{ subItem.descripcion }}</span>
                                                </a>
                                            </li>
                                        </ul>
                                    </button>
                               
                                </div>
                            </template>

                            <!-- Single Menu Item -->
                            <template v-else-if="menuItem.idOpcion !== 18 && menuItem.subopciones.length === 0">
                                <a href="#"
                                @@click="irAMenu(menuItem.controller, menuItem.pagina)"
                                   class="btn btn-link btn-block text-left">
                                    <i :class="menuItem.icono + ' vsm-icon'"></i>
                                    <span>{{ menuItem.descripcion }}</span>
                                </a>
                            </template>

                            <!-- Default Menu Item -->
                            <template v-else>
                                <a href="#"
                                @@click="irAMenu(menuItem.controller, menuItem.pagina)"
                                   class="btn btn-link btn-block text-left">
                                    <i :class="menuItem.icono + ' vsm-icon'"></i>
                                    <span>{{ menuItem.descripcion }}</span>
                                </a>
                            </template>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
   


    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
    <script src="https://cdn.jsdelivr.net/npm/flatpickr"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/luxon/3.5.0/luxon.min.js"></script>

    <partial name="Alertas" />

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/axios/axios.js"></script>
    @* <script src="~/lib/otros/FuncionesGenerales.js" asp-append-version="true"></script> *@
    <script src="~/lib/vue/vue.global.prod.js"></script>
    <script src="~/js/funciones-generales.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/tinymce/tinymce.min.js"></script>

    <!-- User Content  -->
    <div id="content">
        <div class="w-100 h-100">
            @RenderBody()
        </div>
    </div>

</body>
</html>

